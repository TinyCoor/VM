%include "../src/vm_lib.h"

%label N     30
%label print_u64 4

jmp main

main:
      push 0
      push 1
      push N
loop:
      swap 2
      dup 0
      ffi print_u64
      dup 1
      addi
      swap 1
      swap 2
      push 1
      subi

      dup 0
      push 0
      eq
      not

      jmp_if loop
	  halt

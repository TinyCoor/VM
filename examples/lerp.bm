
jmp main

lerpf:
    dup 3
    dup 3
    dup 1
    subf
    dup 3
    mulf
    addf

#清理栈 clean up
    swap 2
    drop
    swap 2
    drop
    swap 2
    drop
    ret

main:
    push 69.0 #15
    push 420.0
    push 0.0
    call lerpf
    ffi 2

    push 69.0  #18
    push 420.0
    push 1.0
    call lerpf
    ffi 2

    push 69.0
    push 420.0
    push 0.5
    call lerpf
    ffi 2
    halt
